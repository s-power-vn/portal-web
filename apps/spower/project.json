{
  "name": "spower",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/spower/src",
  "projectType": "application",
  "tags": [],
  "options": {
    "postcssConfig": "apps/spower/postcss.config.mjs"
  },
  "targets": {
    "dev": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "outputs": ["{workspaceRoot}/dist/apps/spower"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/spower",
        "command": "vinxi dev"
      },
      "configurations": {}
    },
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "outputs": ["{workspaceRoot}/dist/apps/spower"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/spower",
        "command": "vinxi build"
      },
      "configurations": {}
    }
  }
}
